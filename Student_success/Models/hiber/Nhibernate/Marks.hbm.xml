<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="student_success" namespace="Student_success.Models">
  <class name="Marks" table="Marks" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="MarkDate" />
    <property name="Mark" />
    <many-to-one name="Student" column="StudentId" />
    <many-to-one name="Subjects" column="SubjectsId" />
  </class>
</hibernate-mapping>
