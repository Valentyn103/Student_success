<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="student_success" namespace="Student_success.Models">
  <class name="Student" table="Student" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Name" />
    <property name="Surname" />
    <property name="Email" />
    <property name="Number" />
    <many-to-one name="Groups" column="GroupsId" />
    <list name="Marks" cascade="all-delete-orphan">
      <key column="StudentId"/>
      <index column="MarkIndex" />
      <one-to-many class="Marks"/>
    </list>
  </class>
</hibernate-mapping>
